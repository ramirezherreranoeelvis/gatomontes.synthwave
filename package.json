{
	"name": "synthwave-dark-blue",
	"displayName": "SynthWave '84 Dark Blue",
	"description": "A darker shade in SynthWave '84 by robb0wen, a Synthwave-inspired color theme to satisfy your neon dreams",
	"version": "0.0.1",
	"author": "Gatomontes",
	"publisher": "GatomontesRoseIII",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ramirezherreranoeelvis/synthwave-vscode-dark-blue"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"extensionKind": [
		"ui"
	],
	"keywords": [
		"retro",
		"80s",
		"synthwave"
	],
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"onCommand:synthwave84.enableNeon",
		"onCommand:synthwave84.disableNeon"
	],
	"main": "./src/extension.js",
	"contributes": {
		"themes": [
			{
				"label": "SynthWave '84",
				"uiTheme": "vs-dark",
				"path": "./themes/synthwave-color-theme.json"
			}
		],
		"commands": [
			{
				"command": "synthwave84.enableNeon",
				"title": "Synthwave '84: Enable Neon Dreams"
			},
			{
				"command": "synthwave84.disableNeon",
				"title": "Synthwave '84: Disable Neon Dreams"
			}
		],
		"configuration": {
			"title": "Synthwave '84",
			"properties": {
				"synthwave84.brightness": {
					"type": "number",
					"default": 0.45,
					"description": "Set the brightness value for the glow effect. 0.0 is fully transparent and 1.0 is fully bright"
				},
				"synthwave84.disableGlow": {
					"type": "boolean",
					"default": false,
					"description": "Disable the glow effect, but show Synthwave '84 editor chrome updates"
				}
			}
		},
		"languages": [
			{
				"id": "html-in-js-template",
				"aliases": [
					"HTML in JS Template",
					"html-in-js-template"
				],
				"extensions": [
					".js",
					".ts"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "html-in-js-template",
				// "scopeName": "text.html.js.template",
				"path": "./syntaxes/template-html.json",
				"embeddedLanguages": {
					// "source.js": "javascript"
				}
			}
		],
		"activationEvents": [
			"onLanguage:html-in-js-template"
		]
	},
	"__metadata": {
		"id": "e5fd2b56-1637-4d4f-8252-6c9d416f9a28",
		"publisherId": "561257c5-26a1-41f1-944f-17639b7b9c87",
		"publisherDisplayName": "Robb Owen",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1729703269773,
		"pinned": false,
		"preRelease": false,
		"source": "gallery"
	}
}